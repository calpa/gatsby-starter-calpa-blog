{
  "name": "blog",
  "description": "Calpa's Blog",
  "version": "1.0.0",
  "author": "Calpa Liu <calpaliu@gmail.com>",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.4",
    "@fortawesome/free-brands-svg-icons": "^5.3.1",
    "@fortawesome/free-regular-svg-icons": "^5.3.1",
    "@fortawesome/free-solid-svg-icons": "^5.3.1",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "axios": "^0.18.0",
    "babel-loader": "^8.0.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "chalk": "^2.4.1",
    "dayjs": "^1.7.5",
    "dotenv": "^6.0.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "gatsby": "^2.0.76",
    "gatsby-link": "^2.0.7",
    "gatsby-plugin-catch-links": "^2.0.9",
    "gatsby-plugin-feed": "^2.0.11",
    "gatsby-plugin-layout": "^1.0.11",
    "gatsby-plugin-manifest": "^2.0.12",
    "gatsby-plugin-netlify": "^2.0.6",
    "gatsby-plugin-nprogress": "^2.0.7",
    "gatsby-plugin-offline": "^2.0.20",
    "gatsby-plugin-react-helmet": "^3.0.5",
    "gatsby-plugin-sass": "^2.0.7",
    "gatsby-plugin-sentry": "^1.0.0",
    "gatsby-plugin-sitemap": "^2.0.3",
    "gatsby-plugin-webpack-bundle-analyzer": "^1.0.3",
    "gatsby-source-filesystem": "^2.0.12",
    "gitalk": "^1.4.0",
    "han-css": "^3.3.0",
    "highlight.js": "^9.12.0",
    "lozad": "^1.6.0",
    "md5": "^2.2.1",
    "node-sass": "^4.9.3",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-ga": "^2.5.3",
    "react-helmet": "^5.2.0",
    "react-transition-group": "^2.5.2",
    "remarkable": "^1.7.1",
    "rimraf": "^2.6.2",
    "smooth-scroll": "^15.0.0",
    "webpack": "^4.19.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "lint-staged"
    }
  },
  "scripts": {
    "start": "npm run fetch:config && npm run develop",
    "build": "npm run fetch:config && gatsby build --prefix-paths",
    "fetch:config": "NODE_ENV=development node ./scripts/index.js",
    "postbuild": "rimraf public/**/*.map",
    "develop": "gatsby develop",
    "develop:remote": "gatsby develop -p 8080 -H 0.0.0.0",
    "deploy": "npm run build && gh-pages -d public",
    "format": "eslint --fix --ignore-path .gitignore .",
    "format-style": "stylelint src/**/*.scss --syntax=scss",
    "test": "echo \"Error: no test specified\" && exit 1",
    "functions": "netlify-lambda serve functions",
    "reset": "rimraf pubilc/ .cache/",
    "commit": "npx git-cz",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "revert": "node ./scripts/revertConfig.js"
  },
  "devDependencies": {
    "babel-plugin-transform-runtime": "^6.23.0",
    "eslint": "^5.6.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-react": "^7.11.1",
    "gh-pages": "^2.0.0",
    "husky": "^1.0.0-rc.14",
    "lint-staged": "^8.0.4",
    "prettier": "^1.7.4",
    "stylelint": "^9.5.0",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-scss": "^3.4.0",
    "@storybook/react": "^4.0.2",
    "@storybook/addon-actions": "^4.0.2",
    "@storybook/addon-links": "^4.0.2",
    "@storybook/addons": "^4.0.2",
    "@babel/core": "^7.1.2"
  },
  "lint-staged": {
    "linters": {
      "**/*.js": [
        "eslint --fix",
        "git add"
      ],
      "**/*.css": "stylelint",
      "**/*.scss": "stylelint --syntax=scss"
    },
    "ignore": [
      "**/dist/*.min.js"
    ]
  }
}
